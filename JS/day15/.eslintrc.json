{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "plugins": [
        "prettier",
        "jsdoc"
    ],
    "rules": {
      "prettier/prettier": "error",
      "jsdoc/valid-types": "off"
    },
    "extends": [
        "plugin:jsdoc/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    }
}
